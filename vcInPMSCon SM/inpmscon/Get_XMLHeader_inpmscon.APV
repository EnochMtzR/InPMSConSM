[DEFINITION]
[SOURCE]
PROPERTY:BEGIN
PRIORITY 4000
PROPERTY:END
!IF NOT XML:CreateXMLString()
IF NOT XML:CreateXMLString(,,FALSE,'')
    XML:CreateParent('soap:Header');XML:AddParent()
        XML:CreateParent('wsse:Security')
            XML:CreateAttribute('soap:mustUnderstand','1')
            XML:CreateAttribute('xmlns:wsse','http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd')
            XML:AddParent()
            XML:CreateParent('wsse:UsernameToken');XML:AddParent()
                XML:AddElement('wsse:Username',CLIP(LEFT(GCredentials.username)))
                XML:CreateParent('wsse:Password')
                XML:CreateAttribute('Type','http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-username-token-profile-1.0#PasswordText')
                XML:AddParent(True,CLIP(LEFT(GCredentials.password)))
            XML:CloseParent('wsse:UsernameToken')
        XML:CloseParent('wsse:Security')
    XML:CloseParent('soap:Header')
    XML:CloseParent()
    XMLHeader = XML:CloseXMLString()
END
RETURN XMLHeader




