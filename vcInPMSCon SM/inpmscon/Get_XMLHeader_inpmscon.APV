! STRING(@s255),USE(XMLHeader),DECIMAL(14)
XMLHeader                CSTRING(8001)
!!> GUID('d9c8075c-beed-4bb9-b9ec-9240f438d602'),PROMPT('XMLH eader:'),HEADER('XMLH eader'),PICTURE(@s255),TYPEMODE(INS)
[PROMPTS]
%GenerateOpenClose LONG  (0)
